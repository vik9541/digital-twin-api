# Personal Assistant Bot - Environment Variables
# Скопируй в .env и заполни значения

# Telegram Bot
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here

# Google Cloud Vision (OCR)
# Можно указать путь к файлу credentials или оставить пустым
GOOGLE_APPLICATION_CREDENTIALS=path/to/credentials.json

# Yandex Market API (опционально)
YANDEX_MARKET_API_KEY=your_yandex_api_key_here

# Режим отладки
DEBUG=false
